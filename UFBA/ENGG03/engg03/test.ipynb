   "source": [
    "import sympy as sp\n",
    "\n",
    "x: sp.Symbol = sp.symbols(\"x\")\n",
    "q0 = 1\n",
    "t = 0.05\n",
    "C = 10**-4\n",
    "L = 5\n",
    "\n",
    "from sympy.plotting import plot\n",
    "\n",
    "(\n",
    "    q0\n",
    "    * sp.exp((x * t) / (2 * L))\n",
    "    * sp.cos(t * sp.sqrt((1 / (L * C)) - (x / (2 * L))**2 ))\n",
    ")\n"
   ]
  }
